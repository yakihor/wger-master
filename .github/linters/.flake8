[flake8]
# H101: Use TODO(NAME)
# W503: line break before binary operator
ignore = H101,W503
max-line-length = 100

# Removed migrations from exclude list since github's superlinter lints them anyway
exclude =
    extras,
    build,
    dist,
    yarn,
    settings.py,
    docs,
    urls.py : E501
    apps.py : F401
